{\n  "data_sources": [\n    {\n      "file_name": "housetrain.csv",\n      "description": "Data from a house pricing competition, contains information about various house attributes including measurements.",\n    }\n  ],\n  "data_variables": [\n    {\n      "variable_name": "df",\n      "description": "Pandas DataFrame containing all house attributes loaded from the CSV file."\n    },\n    {\n      "variable_name": "df_float",\n      "description": "DataFrame containing only the floating-point attributes of the original DataFrame."\n    },\n    {\n      "variable_name": "lotFrontage",\n      "description": "Selected measure for the lot frontage attribute from df_float."\n    },\n    {\n      "variable_name": "masVnrAre",\n      "description": "Selected measure for the masonry veneer area attribute from df_float."\n    },\n    {\n      "variable_name": "garageYrBlt",\n      "description": "Selected measure for the garage year built attribute from df_float."\n    },\n    {\n      "variable_name": "salePrice",\n      "description": "Target variable representing the sale price of the house from the original DataFrame."\n    },\n    {\n      "variable_name": "lr1",\n      "description": "Linear regression model for predicting SalePrice based on LotFrontage."\n    },\n    {\n      "variable_name": "lr2",\n      "description": "Linear regression model for predicting SalePrice based on MasVnrArea."\n    },\n    {\n      "variable_name": "lr3",\n      "description": "Linear regression model for predicting SalePrice based on GarageYrBlt."\n    }\n  ],\n  "data_flow": [\n    {\n      "variable": "df",\n      "created_by": "pd.read_csv(url)",\n      "used_in": ["df_float", "lotFrontage", "masVnrAre", "garageYrBlt", "salePrice"]\n    },\n    {\n      "variable": "df_float",\n      "created_by": "df.select_dtypes(include=['float64']).copy()",\n      "used_in": ["lotFrontage", "masVnrAre", "garageYrBlt"]\n    },\n    {\n      "variable": "lotFrontage",\n      "created_by": "df_float[['LotFrontage']]",\n      "used_in": ["lr1", "plt.subplot(1, 3, 1)"]\n    },\n    {\n      "variable": "masVnrAre",\n      "created_by": "df_float[['MasVnrArea']]",\n      "used_in": ["lr2", "plt.subplot(1, 3, 2)"]\n    },\n    {\n      "variable": "garageYrBlt",\n      "created_by": "df_float[['GarageYrBlt']]",\n      "used_in": ["lr3", "plt.subplot(1, 3, 3)"]\n    },\n    {\n      "variable": "salePrice",\n      "created_by": "df['SalePrice']",\n      "used_in": ["lr1","lr2","lr3","mean_squared_error"]\n    },\n    {\n      "variable": "lr1",\n      "created_by": "LinearRegression().fit(lotFrontage, salePrice)",\n      "used_in": ["print"]\n    },\n    {\n      "variable": "lr2",\n      "created_by": "LinearRegression().fit(masVnrAre, salePrice)",\n      "used_in": ["print"]\n    },\n    {\n      "variable": "lr3",\n      "created_by": "LinearRegression().fit(garageYrBlt, salePrice)",\n      "used_in": ["print"]\n    }\n  ],\n  "models": [\n    {\n      "model_name": "Linear Regression",\n      "input_features": ["LotFrontage"],\n      "target_variable": "SalePrice",\n      "hyperparameters": {}\n    },\n    {\n      "model_name": "Linear Regression",\n      "input_features": ["MasVnrArea"],\n      "target_variable": "SalePrice",\n      "hyperparameters": {}\n    },\n    {\n      "model_name": "Linear Regression",\n      "input_features": ["GarageYrBlt"],\n      "target_variable": "SalePrice",\n      "hyperparameters": {}\n    }\n  ]\n}