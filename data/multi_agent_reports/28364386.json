{\n  "data_sources": [\n    {\n      "file_name": "churn_prediction.csv",\n      "description": "Contains customer demographic, relationship, and transactional information used for churn prediction."\n    }\n  ],\n  "data_variables": [\n    {\n      "variable_name": "customer_id",\n      "description": "Customer id, uniquely identifying each customer."\n    },\n    {\n      "variable_name": "vintage",\n      "description": "Customer's vintage with the bank in number of days."\n    },\n    {\n      "variable_name": "age",\n      "description": "Customer's age."\n    },\n    {\n      "variable_name": "gender",\n      "description": "Gender of customer."\n    },\n    {\n      "variable_name": "dependents",\n      "description": "Number of dependents of the customer."\n    },\n    {\n      "variable_name": "occupation",\n      "description": "Occupation of the customer."\n    },\n    {\n      "variable_name": "city",\n      "description": "Anonymized city of the customer."\n    },\n    {\n      "variable_name": "customer_nw_category",\n      "description": "Net worth category of the customer (1:High, 2:Medium, 3:Low)."\n    },\n    {\n      "variable_name": "branch_code",\n      "description": "Branch code for the customer's account."\n    },\n    {\n      "variable_name": "days_since_last_transaction",\n      "description": "Number of days since the last transaction in the last year."\n    },\n    {\n      "variable_name": "current_balance",\n      "description": "Customer's current balance."\n    },\n    {\n      "variable_name": "previous_month_end_balance",\n      "description": "End of month balance for the previous month."\n    },\n    {\n      "variable_name": "average_monthly_balance_prevQ",\n      "description": "Average monthly balance in the previous quarter."\n    },\n    {\n      "variable_name": "average_monthly_balance_prevQ2",\n      "description": "Average monthly balance in the quarter before the previous one."\n    },\n    {\n      "variable_name": "current_month_credit",\n      "description": "Total credit amount for the current month."\n    },\n    {\n      "variable_name": "previous_month_credit",\n      "description": "Total credit amount for the previous month."\n    },\n    {\n      "variable_name": "current_month_debit",\n      "description": "Total debit amount for the current month."\n    },\n    {\n      "variable_name": "previous_month_debit",\n      "description": "Total debit amount for the previous month."\n    },\n    {\n      "variable_name": "current_month_balance",\n      "description": "Average balance for the current month."\n    },\n    {\n      "variable_name": "previous_month_balance",\n      "description": "Average balance for the previous month."\n    },\n    {\n      "variable_name": "churn",\n      "description": "Indicates if customer will fall below minimum balance next quarter (1/0)."\n    }\n  ],\n  "data_flow": [\n    {\n      "variable": "train",\n      "created_by": "pd.read_csv('../input/churn-prediction/churn_prediction.csv')",\n      "flows_to": [\n        "train, test = train_test_split(train, test_size=0.2, random_state=42)"\n      ]\n    },\n    {\n      "variable": "train_",\n      "created_by": "LGBMImputer().fit_transform(train[col])",\n      "flows_to": [\n        "train_['churn'] = train['churn']"\n      ]\n    },\n    {\n      "variable": "test_",\n      "created_by": "LGBMImputer().transform(test[col])",\n      "flows_to": [\n        "test_['churn'] = test['churn']"\n      ]\n    },\n    {\n      "variable": "X_train, y_train",\n      "created_by": "train.pop('churn') and train",\n      "flows_to": [\n        "automl.fit(X_train, y_train, task='classification')"\n      ]\n    }\n  ],\n  "models": [\n    {\n      "model_name": "AutoML",\n      "input_features": "All features except 'churn'",\n      "target_variable": "churn",\n      "hyperparameters": {\n        "metric": "roc_auc",\n        "time_budget": 900\n      }\n    }\n  ]\n}