{\n  "data_sources": [\n    {\n      "file_name": "IPL Matches 2008-2020.csv",\n      "description": "Dataset containing IPL match results from 2008 to 2020."\n    }\n  ],\n  "data_variables": [\n    {\n      "variable_name": "df",\n      "description": "DataFrame containing the full dataset of IPL matches."\n    },\n    {\n      "variable_name": "dx",\n      "description": "DataFrame containing matches played by Chennai Super Kings (CSK)."\n    },\n    {\n      "variable_name": "dy",\n      "description": "Pivot table of match results based on 1st batting or fielding."\n    },\n    {\n      "variable_name": "stat",\n      "description": "Chi-square statistic result from the chi-square test."\n    },\n    {\n      "variable_name": "p",\n      "description": "P-value from the chi-square test."\n    }\n  ],\n  "data_flow": [\n    {\n      "variable_name": "df",\n      "creation": "Loaded using pd.read_csv",\n      "flow": "Filtered to only include matches from 2018 to 2020 and used to create dx."\n    },\n    {\n      "variable_name": "dx",\n      "creation": "Combines matches where CSK is team1 or team2.",\n      "flow": "Used to create dy and perform chi-square test."\n    },\n    {\n      "variable_name": "dy",\n      "creation": "Created using pivot_table to summarize match results based on toss decision.",\n      "flow": "Input for chi-square test."\n    },\n    {\n      "variable_name": "stat",\n      "creation": "Output from chi2_contingency function.",\n      "flow": "Used to determine the result of the statistical test."\n    },\n    {\n      "variable_name": "p",\n      "creation": "Output from chi2_contingency function.",\n      "flow": "Used to conclude the hypothesis test."\n    }\n  ],\n  "models": [\n    {\n      "model_name": "Chi-square test of independence",\n      "input_features": [\n        "1st batting or fielding",\n        "MatchResult"\n      ],\n      "target_variable": "Association between 1st batting/fielding and match results",\n      "hypothesis": {\n        "null": "The result of the match is independent of 1st batting or fielding.",\n        "alternative": "The result of the match is dependent on 1st batting or fielding."\n      },\n      "significance_level": 0.05\n    }\n  ]\n}