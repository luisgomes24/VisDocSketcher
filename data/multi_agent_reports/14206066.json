{\n  "data_sources": [\n    {\n      "file_name": "../input/alzheimers-dataset-4-class-of-images/Alzheimer_s Dataset/train",\n      "description": "Training images for Alzheimer's dataset"\n    },\n    {\n      "file_name": "../input/alzheimers-dataset-4-class-of-images/Alzheimer_s Dataset/test",\n      "description": "Test images for Alzheimer's dataset"\n    }\n  ],\n  "data_variables": [\n    {\n      "variable_name": "train_dataset",\n      "description": "Training dataset for the model, generated from training directory."\n    },\n    {\n      "variable_name": "valid_dataset",\n      "description": "Validation dataset for the model, generated from training directory."\n    },\n    {\n      "variable_name": "test_dataset",\n      "description": "Test dataset for final evaluation of the model."\n    },\n    {\n      "variable_name": "model",\n      "description": "The convolutional neural network model built for classification."\n    },\n    {\n      "variable_name": "history",\n      "description": "Training history, containing metrics of training and validation phases."\n    }\n  ],\n  "data_flow": [\n    {\n      "variable": "train_dataset",\n      "created_by": "train_datagen.flow_from_directory",\n      "flows_to": ["model training"]\n    },\n    {\n      "variable": "valid_dataset",\n      "created_by": "valid_datagen.flow_from_directory",\n      "flows_to": ["model validation"]\n    },\n    {\n      "variable": "test_dataset",\n      "created_by": "test_datagen.flow_from_directory",\n      "flows_to": ["model evaluation"]\n    },\n    {\n      "variable": "model",\n      "created_by": "build_model()",\n      "flows_to": ["model training", "model evaluation"]\n    },\n    {\n      "variable": "history",\n      "created_by": "model.fit",\n      "flows_to": ["result visualization"]\n    }\n  ],\n  "models": [\n    {\n      "model_name": "Sequential Model",\n      "layers": [\n        {\n          "layer_type": "Conv2D",\n          "params": {"filters": 32, "kernel_size": [3,3], "activation": "relu" }\n        },\n        {\n          "layer_type": "MaxPooling2D",\n          "params": {"pool_size": [3,3]}\n        },\n        {\n          "layer_type": "Conv2D",\n          "params": {"filters": 64, "kernel_size": [3,3], "activation": "relu" }\n        },\n        {\n          "layer_type": "MaxPooling2D",\n          "params": {"pool_size": [3,3]}\n        },\n        {\n          "layer_type": "Conv2D",\n          "params": {"filters": 128, "kernel_size": [3,3], "activation": "relu" }\n        },\n        {\n          "layer_type": "MaxPooling2D",\n          "params": {"pool_size": [3,3]}\n        },\n        {\n          "layer_type": "Dense",\n          "params": {"units": 4, "activation": "softmax" }\n        }\n      ],\n      "input_features": "Image data (224x224x3)",\n      "target_variable": "Categorical class labels (4 classes)",\n      "hyperparameters": {"optimizer": "rmsprop", "loss_function": "categorical_crossentropy"}\n    }\n  ]\n}