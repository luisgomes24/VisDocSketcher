{\n  "data_sources": [\n    {\n      "file_path": "../input/salary-dataset/Salary.csv",\n      "description": "Contains salary data and their corresponding years of experience for analysis"\n    }\n  ],\n  "data_variables": [\n    {\n      "name": "data",\n      "description": "DataFrame containing the salary dataset"\n    },\n    {\n      "name": "x",\n      "description": "Feature set containing all columns except 'Salary'"\n    },\n    {\n      "name": "y",\n      "description": "Target variable containing the 'Salary' column"\n    },\n    {\n      "name": "xtrain",\n      "description": "Training set for features after splitting"\n    },\n    {\n      "name": "xtest",\n      "description": "Testing set for features after splitting"\n    },\n    {\n      "name": "ytrain",\n      "description": "Training set for target variable (Salary) after splitting"\n    },\n    {\n      "name": "ytest",\n      "description": "Testing set for target variable (Salary) after splitting"\n    },\n    {\n      "name": "L",\n      "description": "LinearRegression model instance for predicting salaries"\n    },\n    {\n      "name": "y_pred",\n      "description": "Predicted salary values for the test set"\n    }\n  ],\n  "data_flow": [\n    {\n      "variable": "data",\n      "creation_method": "pd.read_csv",\n      "flows_to": ["data exploration", "data visualization", "train_test_split"]\n    },\n    {\n      "variable": "x",\n      "creation_method": "data.drop('Salary', axis=1)",\n      "flows_to": ["train_test_split"]\n    },\n    {\n      "variable": "y",\n      "creation_method": "data['Salary']",\n      "flows_to": ["train_test_split"]\n    },\n    {\n      "variable": "xtrain, xtest, ytrain, ytest",\n      "creation_method": "train_test_split(x, y, test_size=0.2, random_state=42)",\n      "flows_to": ["L.fit()"]\n    },\n    {\n      "variable": "L",\n      "creation_method": "LinearRegression()",\n      "flows_to": ["L.fit()", "y_pred", "L.score"]\n    },\n    {\n      "variable": "y_pred",\n      "creation_method": "L.predict(xtest)",\n      "flows_to": ["Model evaluation"]\n    }\n  ],\n  "models": [\n    {\n      "model_name": "LinearRegression",\n      "input_features": "x (All columns except 'Salary')",\n      "target_variable": "y (Salary)",\n      "hyperparameters": {}\n    }\n  ]\n}