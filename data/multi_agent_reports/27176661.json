{\n  "data_sources": [\n    {\n      "file": "../input/bank-marketing-dataset/bank.csv",\n      "description": "Contains details about bank marketing campaigns including customer demographics, job types, and their bank account balance."\n    }\n  ],\n  "data_variables": [\n    {\n      "name": "df",\n      "description": "DataFrame containing the initial bank marketing dataset processed to include only useful columns."\n    },\n    {\n      "name": "clusters",\n      "description": "Array containing the cluster labels assigned to each data point based on KMeans clustering."\n    },\n    {\n      "name": "df_OH",\n      "description": "DataFrame with one-hot encoded categorical features and standardized continuous variables."\n    },\n    {\n      "name": "groups",\n      "description": "Aggregated DataFrame showing cluster statistics including minimum, median, mean, maximum, and total sum of balances grouped by clusters."\n    }\n  ],\n  "data_flow": [\n    {\n      "variable": "df",\n      "creation": "pd.read_csv('../input/bank-marketing-dataset/bank.csv')",\n      "flow": ["cleaned by dropping certain columns and duplicates", "used in clustering"]\n    },\n    {\n      "variable": "df_OH",\n      "creation": "OneHotEncoder and concatenation with standardized values",\n      "flow": ["used to fit KMeans clustering", "plotted with clusters"]\n    },\n    {\n      "variable": "clusters",\n      "creation": "kmeans.fit_predict(df[cont_features])",\n      "flow": ["added to the df and df_OH DataFrames", "used for grouping in groups"]\n    },\n    {\n      "variable": "groups",\n      "creation": "df.groupby(['cluster']).agg()",\n      "flow": ["used to analyze cluster statistics", "sorted for display purposes"]\n    }\n  ],\n  "models": [\n    {\n      "model": "KMeans",\n      "input_features": "df[cont_features] and df_OH",\n      "target_variable": "clusters",\n      "hyperparameters": {\n        "n_clusters": [4, 5],\n        "init": "k-means++",\n        "n_init": 30,\n        "max_iter": 100,\n        "random_state": 0\n      }\n    }\n  ]\n}