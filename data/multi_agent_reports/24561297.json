{\n    "data_sources": [\n        {\n            "file": "../input/autism-prediction/Autism-prediction/train.csv",\n            "description": "Training dataset containing 800 datapoints with 21 features and 1 target variable related to Autism prediction."\n        },\n        {\n            "file": "../input/autism-prediction/Autism-prediction/test.csv",\n            "description": "Test dataset containing 200 datapoints with 21 features related to Autism prediction."\n        }\n    ],\n    "data_variables": [\n        {\n            "variable": "data_train",\n            "description": "DataFrame containing the training data."\n        },\n        {\n            "variable": "data_test",\n            "description": "DataFrame containing the test data."\n        },\n        {\n            "variable": "X",\n            "description": "Feature set for training model after dropping the target variable."\n        },\n        {\n            "variable": "y",\n            "description": "Target variable (Class/ASD) for the training model."\n        },\n        {\n            "variable": "rfc_model",\n            "description": "Random Forest Classifier model used for prediction."\n        }\n    ],\n    "data_flow": [\n        {\n            "variable": "data_train",\n            "creation": "pd.read_csv",\n            "flows_to": "Analyzed for data understanding and model training."\n        },\n        {\n            "variable": "data_test",\n            "creation": "pd.read_csv",\n            "flows_to": "Used for final predictions after model training."\n        },\n        {\n            "variable": "X",\n            "creation": "data_train.drop('Class/ASD', axis=1)",\n            "flows_to": "Used as feature set for model training."\n        },\n        {\n            "variable": "y",\n            "creation": "data_train['Class/ASD']",\n            "flows_to": "Used as target variable for model training."\n        },\n        {\n            "variable": "rfc_model",\n            "creation": "RandomForestClassifier(n_estimators=150)",\n            "flows_to": "Fitted with X_train and y_train, then used to predict y_pred_test."\n        }\n    ],\n    "models": [\n        {\n            "model": "RandomForestClassifier",\n            "input_features": "X",\n            "target_variable": "y",\n            "hyperparameters": {\n                "n_estimators": 150\n            }\n        }\n    ]\n}