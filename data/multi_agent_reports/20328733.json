{\n  "data_sources": [\n    {\n      "file_name": "/kaggle/input/aapl10y/AAPL10Y.csv",\n      "description": "AAPL stock data for ten years including closing prices."
    }\n  ],\n  "data_variables": [\n    {\n      "variable": "df",\n      "description": "DataFrame containing the AAPL stock data with 'date' set as index."
    },\n    {\n      "variable": "df_week",\n      "description": "Weekly average closing prices from the stock data."
    },\n    {\n      "variable": "udiff",\n      "description": "DataFrame of weekly returns after calculating logs of closing prices."
    },\n    {\n      "variable": "forecast",\n      "description": "Forecasted values using the ARIMA model."
    }\n  ],\n  "data_flow": [\n    {\n      "variable": "df",\n      "created_by": "pd.read_csv",\n      "flows_to": ["df_week"]\n    },\n    {\n      "variable": "df_week",\n      "created_by": "resampling",\n      "flows_to": ["udiff", "plot"]\n    },\n    {\n      "variable": "udiff",\n      "created_by": "log diff of closing prices",\n      "flows_to": ["model_building", "ACF", "PACF", "plot", "forecast"]\n    },\n    {\n      "variable": "forecast",\n      "generated_by": "ar1.forecast",\n      "flows_to": ["plot"]\n    }\n  ],\n  "models": [\n    {\n      "model_type": "ARMA",\n      "features": ["udiff"],\n      "target_variable": "weekly_ret",\n      "hyperparameters": {"p": 3, "q": 1}\n    }\n  ]\n}