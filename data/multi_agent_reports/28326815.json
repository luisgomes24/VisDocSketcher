{\n  "data_sources": [\n    {\n      "file_path": "/kaggle/input/skin-cancer-mnist-ham10000/HAM10000_metadata.csv",\n      "description": "Contains metadata of skin cancer images for classification"\n    }\n  ],\n  "data_variables": [\n    {\n      "name": "skin_df",\n      "description": "DataFrame containing skin cancer metadata, sorted by image_id"\n    },\n    {\n      "name": "dls",\n      "description": "DataLoaders object encapsulating training and validation datasets"\n    }\n  ],\n  "data_flow": [\n    {\n      "variable": "skin_df",\n      "creation_method": "pd.read_csv(csv_path)",\n      "flows_to": ["img_to_class_dict"]\n    },\n    {\n      "variable": "img_to_class_dict",\n      "creation_method": "Mapping image_id to disease name",\n      "flows_to": ["dls"]\n    },\n    {\n      "variable": "dls",\n      "creation_method": "dblock.dataloaders(img_path, bs=64)",\n      "flows_to": ["learn"]\n    }\n  ],\n  "models": [\n    {\n      "model_name": "resnet34",\n      "input_features": "Images from the dataset",\n      "target_variable": "Disease classification",\n      "hyperparameters": {\n        "epochs": 3,\n        "learning_rate": "1e-2"\n      }\n    }\n  ]\n}