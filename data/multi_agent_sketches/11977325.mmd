flowchart TD

    %% Node for data frame creation
    DF["DataFrame 'df'"]
    click DF "" _self
    %% Node for data source (hypothetical)
    DS["Data Source (hypothetical)"]
    click DS "" _self

    
    %% Data Source to DataFrame transformation
    DS -->|"Processed into"| DF

    %% Node for Data Usage in plotting
    PLOT["Plotting Functions"]
    click PLOT "" _self
    
    %% DataFrame to Plotting Function
    DF -->|"Used for plotting"| PLOT

